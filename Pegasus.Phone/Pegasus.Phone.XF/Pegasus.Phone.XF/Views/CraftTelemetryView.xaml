<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Pegasus.Phone.XF.CraftTelemetryView">
	<ContentView.Content>
    <StackLayout VerticalOptions="Center" HorizontalOptions="CenterAndExpand">
      <Label Text="{Binding Data.Humidity}" HorizontalOptions="CenterAndExpand" />
      <Label Text="{Binding Data.AtmosphericPressure}" HorizontalOptions="CenterAndExpand" />
    </StackLayout>
	</ContentView.Content>
</ContentView>
